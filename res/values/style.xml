<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="file_picker_pane_directory">
		<!-- Layout params cannot be specified in a style which will be applied via theming. -->
		<item name="android:padding">12dp</item>
		<item name="android:textSize">30dp</item>
		<item name="android:maxLines">1</item>
		<item name="android:gravity">top</item>
		<item name="android:textColor">@android:color/black</item>
		<item name="android:background">@drawable/solid_empty</item>
		<item name="android:onClick">onClick</item>
	</style>
	<style name="file_picker_pane_file">
		<item name="android:padding">12dp</item>
		<item name="android:textSize">30dp</item>
		<item name="android:maxLines">1</item>
		<item name="android:gravity">top</item>
		<item name="android:textColor">@android:color/black</item>
		<item name="android:background">@drawable/dotted_empty</item>
		<item name="android:onClick">onClick</item>
	</style>
</resources>
